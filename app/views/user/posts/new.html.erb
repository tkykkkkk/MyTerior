<h1>画像投稿フォーム</h1>
<%= form_with model: @post, url: user_posts_path do |f| %>
  
  <%= f.fields_for :photos do |i| %>
  <%= i.file_field :image, accept: "image/*" %>
  <% end %>
  
  <%= f.label :caption %>
  <%= f.text_area :caption %>
  
  <span>間取り<span>
  
  <span>タグ<span>
  
  <%= f.submit '投稿を確認する'%>
<% end %>